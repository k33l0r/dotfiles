[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
[user]
	name = <%= name %>
	email = <%= email %>
	signingkey = <%= signingkey %>
[includeIF "gitdir:~/Code/work/"]
  path = ~/.gitconfig_work
[core]
	excludesfile = ~/.gitignore
[push]
	default = simple
[alias]
	kudos = blame
[commit]
	gpgsign = true
[gpg]
	program = /usr/local/bin/gpg
